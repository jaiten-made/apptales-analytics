# Database Configuration
# PostgreSQL connection string
# Format: postgresql://user:password@localhost:5432/database_name
DATABASE_URL=

# JWT Authentication
# Secret key for signing JWT tokens
# Generate a secure random string (min 32 characters)
JWT_SECRET=

# Application URLs
# Base URL of the main application
# Example: http://localhost:5173 or https://app.example.com
APP_BASE_URL=

# Analytics Tracker
# Base URL for the analytics tracker service
# Example: http://localhost:8000 or https://tracker.example.com
TRACKER_BASE_URL=

# Email Service (Resend)
# API key for Resend email service
# Get your key from: https://resend.com/api-keys
RESEND_API_KEY=

# Email sender address
# Must be a verified sender in your Resend account
# Example: noreply@example.com
RESEND_FROM_EMAIL=

# Provisioning Portal Security
# Admin password for the provisioning portal
# Use a strong, secure password
PROVISIONING_ADMIN_PASSWORD=

# Provisioning Portal URL
# Base URL for the provisioning portal application
# Example: http://localhost:5174 or https://provision.example.com
PROVISIONING_PORTAL_BASE_URL=