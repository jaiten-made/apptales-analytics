generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int       @id @default(autoincrement())
  type        String
  data        EventData @relation(fields: [eventDataId], references: [id])
  createdAt   DateTime  @default(now())
  eventDataId Int
}

model EventData {
  id    Int     @id @default(autoincrement())
  name  String
  Event Event[]
}
