{
  "name": "apptales-tracker",
  "version": "1.0.0",
  "description": "A lightweight CDN script for tracking all events on websites",
  "type": "module",
  "main": "./dist/tracker.umd.js",
  "module": "./dist/tracker.es.js",
  "exports": {
    ".": {
      "import": "./dist/tracker.es.js",
      "require": "./dist/tracker.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "terser": "^5.36.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.6"
  }
}
